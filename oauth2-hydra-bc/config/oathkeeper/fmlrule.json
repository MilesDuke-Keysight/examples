[
  {
    "matches": [
      {
        "url": "myproxy.com/api",
        "methods": ["GET", "POST"],
        "upstream": {
          "preserveHost": true,
          "stripPath": "/api",
          "url": "mybackend.com/api"
        }
      }
    ],
    "authenticators": [
      {
        "handler": "noop"
      },
      {
        "handler": "anonymous"
      },
      {
        "handler": "oauth2Introspection",
        "config": {
          "requiredScopes": [
            "foo",
            "bar"
          ]
        }
      },
      {
        "handler": "oauth2Introspection",
        "config": {
          "requiredScopes": [
            "foo",
            "bar"
          ],
          "endpoint": "some/other/endpoint"
        }
      },
      {
        "authenticator": "oauth2ClientCredentials"
      }
    ],
    "authorizer": {
      "handler": "ketoWarden",
      "config": {
        "requiredResource": "foo:bar",
        "requiredAction": "foo:bar"
      }
    },
    "session": {
      "handler": "noop|idToken"
    }
  }
]